		      "titleNView":false  //去掉app+h5顶部导航
		    }
		  }
		},
		{
			"path" : "pages/class/class",
			"style" : 
			{
				"navigationBarTitleText" : "人脸识别考勤系统"
			},
			"app-plus":
			{
				"titleNView":false  //去掉app+h5顶部导航
			}
		},
		{
			"path" : "pages/student/student",
			"style" : 
			{
				"navigationBarTitleText" : "班级学生信息",
				"enablePullDownRefresh" : false
			},
			"app-plus":
			{
				"titleNView":false  //去掉app+h5顶部导航
			}
		},
		{
			"path" : "pages/history/history",
			"style" : 
			{
				"navigationBarTitleText" : "考勤历史",
				"enablePullDownRefresh" : false
			},
			"app-plus":
			{
				"titleNView":false  //去掉app+h5顶部导航
			}
		},
		{
			"path" : "pages/mine/mine",
			"style" : 
			{
				"navigationBarTitleText" : "个人中心",
				"enablePullDownRefresh" : false
			}
		}
	],
